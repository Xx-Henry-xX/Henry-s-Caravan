config_version=4

_global_script_classes=[ {
"base": "RichTextEffect",
"class": "RichTextVAlign",
"language": "GDScript",
"path": "res://Script/valign.gd"
} ]
_global_script_class_icons={
"RichTextVAlign": ""
}

[application]

config/name="Henry's Caravan"
run/main_scene="res://Scenes/Title.tscn"
boot_splash/show_image=false
config/icon="res://icon.png"

[autoload]

AudioManager="*res://Scenes/AudioManager.tscn"
GameManager="*res://Scenes/GameManager.tscn"
DefaultControlsSaver="*res://addons/ActionBinding/DefaultControlsSaver.tscn"

[display]

window/size/width=240
window/size/height=320
window/size/resizable=false
window/size/test_width=720
window/size/test_height=960
window/vsync/use_vsync=false
window/handheld/orientation="portrait"
window/stretch/mode="viewport"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "res://addons/SpriteSheetImporter/plugin.cfg" )

